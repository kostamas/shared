{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "shared-ui-components-lib": {
      "root": "projects/shared-ui-components-lib",
      "sourceRoot": "projects/shared-ui-components-lib/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/shared-ui-components-lib/tsconfig.lib.json",
            "project": "projects/shared-ui-components-lib/ng-package.json"
          }
        }
      }
    }
  },
  "defaultProject": "shared-ui-components-lib"
}
